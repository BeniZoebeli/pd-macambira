#!/usr/bin/make -f

all:
	make -C fluid
	make -C flext-iiwu
	make -C shabby
	make -C syncgrain

clean:
	make -C fluid clean
	make -C flext-iiwu clean
	make -C shabby clean
	make -C syncgrain clean

install: 
	make -C fluid install
	make -C flext-iiwu install
	make -C shabby install 
	make -C syncgrain install 
				
